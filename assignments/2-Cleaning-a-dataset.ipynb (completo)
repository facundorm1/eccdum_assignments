      "source": [
        "import re\n",
        "def clean_price(df):\n",
        "    \"\"\"\n",
        "    In this function we will transform the\n",
        "    `price` column into a column of floats.\n",
        "    In case a product has more than one price,\n",
        "    return the lowest one.\n",
        "    \"\"\"\n",
        "\n",
        "    df_3= df_unified.copy()\n",
        "\n",
        "   # Función para extraer el menor de los precios\n",
        "    def extract_min_price(price_str):\n",
        "        # expresión regular para encontrar todos los números (incluyendo decimales)\n",
        "        numbers = re.findall(r'\\d+\\.?\\d*', price_str)\n",
        "        if numbers:\n",
        "            return min(map(float, numbers))\n",
        "        return None\n",
        "\n",
        "    df_3['price'] = df_3['price'].apply(extract_min_price)\n",
        "\n",
        "    df_3['price'] = df_3['price'].astype(float)\n",
        "    return df_3\n"
      ]
    },
    {
      "cell_type": "code",
